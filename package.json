{
  "name": "@hexsoftstudio/theme-default",
  "version": "0.0.2",
  "description": "hexsoftstudio theme default",
  "homepage": "https://github.com/hexsoftstudio/theme-default#readme",
  "bugs": {
    "url": "https://github.com/hexsoftstudio/theme-default/issues"
  },
  "repository": "hexsoftstudio/theme/default",
  "main": "index.js",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass/styles/ -o styles/",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "precommit": "lint-staged"
  },
  "keywords": [
    "Hexsoftstudio"
  ],
  "author": "SirKitts",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.9.2"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.19.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
